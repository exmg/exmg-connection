{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
          "impliedStrict": true
      }
  },
  "env": {
    "es6": true
  },
  "rules": {
    "max-len": ["error", 120],
    "no-console": "off",
    "require-jsdoc": "off",
    "valid-jsdoc": "off"
  }
}
